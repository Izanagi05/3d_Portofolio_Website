{
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/index.html"
        }
      ],
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  